{
    "_description": "Feed-forward FNO solver",
    "use_deq": false,
    "use_micro": true,
    "use_strain": false,
    "use_bc_strain": true,
    "use_stress": false,
    "use_stress_polarization": false,
    "use_energy": false,
    "add_fft_encoding": false,
    "enforce_mean": true,
    "scale_output": true,
    "add_bcs_to_iter": true,
    "fno_args": {
        "modes": [
            -1,
            -1
        ],
        "activ_type": "gelu",
        "init_weight_scale": 0.1,
        "normalize": true,
        "use_weight_norm": true,
        "resid_conn": true,
        "latent_channels": 24,
        "normalize_inputs": false,
        "projection_channels": 128,
        "use_mlp_lifting": false,
        "init_type": "uniform"
    },
    "lr_max": 5e-3
}
